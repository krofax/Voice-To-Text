{
  "name": "speech-recognition",
  "version": "1.0.0",
  "description": "A speech recognition app",
  "main": "script.js",
  "scripts": {
    "start": "browser-sync start --directory --server --files \"*.css, *.html, *.js\"",
    "dev": "npm run watch:css && parcel index.html",
    "build": "parcel build index.html",
    "watch:css": "postcss css/tailwind.css -o css/style.css -w"
