  "license": "ISC",
  "devDependencies": {
    "babel-plugin-transform-runtime": "^6.23.0",
    "browser-sync": "^2.26.7",
    "parcel-bundler": "^1.12.4"
  },
  "dependencies": {
    "@babel/core": "^7.7.7",
    "@babel/polyfill": "^7.7.0",
    "axios": "^0.21.1",
    "tailwindcss": "^1.2.0"
  }
}
